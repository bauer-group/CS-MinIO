{
  "policies": [
    {
      "name": "pAdministrators",
      "statements": [
        {
          "Effect": "Allow",
          "Action": ["admin:*"],
          "Resource": ["arn:aws:s3:::*"]
        },
        {
          "Effect": "Allow",
          "Action": ["s3:*"],
          "Resource": ["arn:aws:s3:::*"]
        }
      ]
    }
  ],
  "users": [
    {
      "access_key": "",
      "secret_key": "${CONSOLE_PASSWORD}",
      "groups": ["gAdministrators"]
    }
  ],
  "groups": [
    {
      "name": "gAdministrators",
      "policies": ["pAdministrators"]
    }
  ]
}
